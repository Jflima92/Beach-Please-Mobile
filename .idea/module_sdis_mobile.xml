<?xml version="1.0" encoding="UTF-8"?>
<project name="module_sdis_mobile" default="compile.module.sdis_mobile">
  <dirname property="module.sdis_mobile.basedir" file="${ant.file.module_sdis_mobile}"/>
  
  <property name="module.jdk.home.sdis_mobile" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.sdis_mobile" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.sdis_mobile" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.sdis_mobile" value="-encoding windows-1252 -source 1.3 ${compiler.args}"/>
  
  <property name="sdis_mobile.output.dir" value="undefined"/>
  <property name="sdis_mobile.testoutput.dir" value="undefined"/>
  
  <path id="sdis_mobile.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="sdis_mobile.module.production.classpath">
    <path refid="library.sdis_mobile_node_modules.classpath"/>
  </path>
  
  <path id="sdis_mobile.runtime.production.module.classpath">
    <path refid="library.sdis_mobile_node_modules.classpath"/>
  </path>
  
  <path id="sdis_mobile.module.classpath">
    <path refid="library.sdis_mobile_node_modules.classpath"/>
  </path>
  
  <path id="sdis_mobile.runtime.module.classpath">
    <path refid="library.sdis_mobile_node_modules.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.sdis_mobile">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.sdis_mobile">
    <patternset refid="excluded.from.module.sdis_mobile"/>
  </patternset>
  
  
  <target name="compile.module.sdis_mobile" depends="compile.module.sdis_mobile.production,compile.module.sdis_mobile.tests" description="Compile module SDIS_Mobile"/>
  
  <target name="compile.module.sdis_mobile.production" depends="register.custom.compilers" description="Compile module SDIS_Mobile; production classes"/>
  
  <target name="compile.module.sdis_mobile.tests" depends="register.custom.compilers,compile.module.sdis_mobile.production" description="compile module SDIS_Mobile; test classes" unless="skip.tests"/>
  
  <target name="clean.module.sdis_mobile" description="cleanup module">
    <delete dir="${sdis_mobile.output.dir}"/>
    <delete dir="${sdis_mobile.testoutput.dir}"/>
  </target>
</project>